<p id="notice"><%= notice %></p>

<h1>Request Ratings</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Skill</th>
      <th>Teacher</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @request_ratings.each do |request_rating| %>
      <tr>
        <td><%= request_rating.student_id %></td>
        <td><%= request_rating.skill_id %></td>
        <td><%= request_rating.teacher_id %></td>
        <td><%= request_rating.status %></td>
        <td><%= link_to 'Show', request_rating %></td>
        <td><%= link_to 'Edit', edit_request_rating_path(request_rating) %></td>
        <td><%= link_to 'Destroy', request_rating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request Rating', new_request_rating_path %>
